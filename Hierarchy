$0 -- the most basic packages
xreflect            (x)        <<<
xtesting            (xreflect) <<<

$1 -- packages that depend on $0 libraries
xcolor              (xtesting) <<<
xcondition          (xreflect, xtesting)
xerror              (xtesting) <<<
xgopool             (xtesting)
xnumber             (xtesting)
xpointer            (xtesting)
xorderedmap         (xreflect, xtesting)
xruntime            (xtesting)
xslice              (xtesting)
xstatus             (xtesting)
xstring             (xtesting)
xtime               (xtesting)

$2 -- packages that depend on $0 and $1 libraries
xmodule             (xcolor, xerror, xtesting)

//////////////////////////////////////////////////////////

$$ -- seperated module, does not depend on any $* packages
xgeneric/...

$$0 -- the most basic packages
xgeneric/xsugar     (x) <<<
xgeneric/xtuple     (x) <<<

$$1 -- packages that depend on $$0 libraries
xgeneric/xgmap      (xgeneric/xsugar, xgeneric/xtuple)
xgeneric/xgslice    (xgeneric/xsugar, xgeneric/xtuple)
